{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/main/main.html" },
    { "src": "/second", "dest": "/second/second.html" },
    { "src": "/api/(.*)", "dest": "/api/$1.js" }
  ]
}
